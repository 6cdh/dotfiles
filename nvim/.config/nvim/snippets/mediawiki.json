{
  "Code": {
    "prefix": "<code>",
    "body": "<code>$1</code>",
    "description": "code snippet"
  },
  "Math": {
    "prefix": "<math>",
    "body": "<math>$1</math>",
    "description": "inline math snippet"
  },
  "SyntaxHighlight": {
    "prefix": "<syntaxhighlight>",
    "body": ["<syntaxhighlight lang=\"$1\">", "$2", "</syntaxhighlight>"]
  },
  "Tabber": {
    "prefix": "<tabber>",
    "body": ["<tabber>", "$2", "</tabber>"]
  },
  "HTML": {
      "prefix": ["html", "tag"],
      "body": ["<$1>", "$2", "</$1>"]
  }
}
